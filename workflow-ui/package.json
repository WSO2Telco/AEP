{
  "name": "workflow-manager-ui",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "serve": "ng server --proxy-config proxy.conf.json  & node server/index.js",
    "serve:debug": "ng server --proxy-config proxy.conf.json  & node-debug server/index.js",
    "ng": "ng serve",
    "start": "ng build && node server/index.js &",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "build":"ng build",
    "bundle": "rimraf bundle/* & ng build  && cp -R server bundle/server && cp package-server.json bundle && mv bundle/package-server.json bundle/package.json"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "animate.css": "^3.5.2",
    "boom": "^4.2.0",
    "bootstrap": "^3.3.7",
    "chart.js": "^2.4.0",
    "core-js": "^2.4.1",
    "glue": "^4.1.0",
    "good": "^7.1.0",
    "good-console": "^6.2.0",
    "h2o2": "^5.4.0",
    "hapi": "^16.1.0",
    "inert": "^4.1.0",
    "material-design-icons": "^3.0.1",
    "moment": "^2.17.1",
    "ng2-bootstrap": "^1.2.6",
    "ng2-charts": "^1.5.0",
    "ng2-slim-loading-bar": "^2.3.0",
    "ng2-toasty": "^2.3.0",
    "q": "^1.4.1",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "wreck": "^10.0.0",
    "xml2js": "^0.4.17",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.3.1",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "angular-cli": "1.0.0-beta.25.5",
    "codelyzer": "~2.0.0-beta.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "node-sass": "^4.3.0",
    "protractor": "~4.0.13",
    "rimraf": "^2.5.4",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.0.3"
  }
}
